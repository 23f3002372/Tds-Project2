{
    "buildCommand": "uv sync --frozen",
    "outputDirectory": ".vercel/output",
    "env": {
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "true",
        "PIP_NO_CACHE_DIR": "1"
    },
    "functions": {
        "api/**/*.py": {
            "maxDuration": 300,
            "memory": 3008,
            "runtime": "python3.12"
        }
    },
    "devCommand": "uv run main.py"
}